{
    "name": "pinnacle/openid-connect",
    "description": "library for use in handling OAuth requests",
    "type": "library",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "myclabs/php-enum": "^1.8",
        "guzzlehttp/guzzle": "^7.3",
        "pinnacle/common-value-objects": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "vimeo/psalm": "^4.7",
        "slevomat/coding-standard": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Pinnacle/OpenidConnect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pinnacle/OpenidConnect\\Tests\\": "tests/"
        }
    },
    "repositories": {
        "common-value-objects": {
            "type": "vcs",
            "url": "https://github.com/pinnacleqi/common-value-objects"
        },
        "coding-standard": {
            "type": "vcs",
            "url": "https://github.com/pinnacleqi/coding-standard"
        }
    }
}
