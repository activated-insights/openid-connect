{
  "name": "pinnacle/openid-connect",
  "description": "Library for OpenID Connect 1.0 relying party (RP) use.",
  "type": "library",
  "license": "proprietary",
  "require": {
    "php": "~8.0.0 || ~8.1.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "pinnacle/common-value-objects": "^1.10",
    "psr/log": "^3.0",
    "myclabs/php-enum": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "Pinnacle\\OpenIdConnect\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pinnacle\\OpenIdConnect\\Tests\\": "tests/"
    }
  },
  "repositories": {
    "common-value-objects": {
      "type": "vcs",
      "url": "https://github.com/pinnacleqi/common-value-objects"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.5",
    "firebase/php-jwt": "^6.0"
  }
}
